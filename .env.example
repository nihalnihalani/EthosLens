# OpenAI API Configuration
VITE_OPENAI_API_KEY=your_openai_api_key_here

# Perplexity API Configuration
VITE_PERPLEXITY_API_KEY=your_perplexity_api_key_here
VITE_PERPLEXITY_MODEL=sonar

# Optional: Custom API endpoint
VITE_PERPLEXITY_API_URL=https://api.perplexity.ai/chat/completions

# Neo4j Configuration
VITE_NEO4J_URI=your_neo4j_uri_here
VITE_NEO4J_USERNAME=your_neo4j_username_here
VITE_NEO4J_PASSWORD=your_neo4j_password_here
VITE_NEO4J_DATABASE=your_database_name_here
VITE_AURA_INSTANCEID=your_instance_id_here
VITE_AURA_INSTANCENAME=your_instance_name_here

# Security Configuration
VITE_RATE_LIMIT_REQUESTS=10
VITE_RATE_LIMIT_WINDOW=60000
VITE_MAX_PROMPT_LENGTH=5000
